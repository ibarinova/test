<font-mappings>
    <font-table>
        <!-- Backwards compatibility aliases -->
        <aliases>
            <alias name="serif">Serif</alias>
        </aliases>
        <aliases>
            <alias name="sans-serif">Sans</alias>
        </aliases>
        <aliases>
            <alias name="monospace">Monospaced</alias>
        </aliases>
        <aliases>
            <alias name="Helvetica">Normal</alias>
        </aliases>
        <aliases>
            <alias name="Courier">Monospaced</alias>
        </aliases>

        <logical-font name="Barcode39">
            <physical-font char-set="default">
                <font-face>Free 3 of 9</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="Sans">
            <physical-font char-set="default">
                <font-face>DellReplica-Regular</font-face>
            </physical-font>
            <!--ja-->
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <!--ko-->
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <!--ru-->
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <!--zh_CN-->
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <!--zh_TW-->
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
            <!-- Suite/EMC   SOW7  19-Feb-2013   Added font-mappings for symbols to handle superscripting START - rs -->
            <physical-font char-set="SymbolsSuperscript">
                <font-face>DellReplica-Regular</font-face>
                <baseline-shift>20%</baseline-shift>
                <override-size>smaller</override-size>
            </physical-font>
            <physical-font char-set="SymbolsService">
                <font-face>DellReplica-Regular</font-face>
            </physical-font>
            <!-- Suite/EMC   SOW7  19-Feb-2013   Added font-mappings for symbols to handle superscripting END - rs -->
        </logical-font>

        <logical-font name="SansLFCaps">
            <physical-font char-set="default">
                <font-face>DellReplica-Regular</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="SansLFRoman">
            <physical-font char-set="default">
                <font-face>DellReplica-Regular</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
            <!-- Suite/EMC   SOW7  19-Feb-2013   Added font-mappings for symbols to handle superscripting START - rs -->
            <physical-font char-set="SymbolsSuperscript">
                <font-face>DellReplica-Regular</font-face>
                <baseline-shift>20%</baseline-shift>
                <override-size>smaller</override-size>
            </physical-font>
            <physical-font char-set="SymbolsService">
                <font-face>DellReplica-Regular</font-face>
            </physical-font>
            <!-- Suite/EMC   SOW7  19-Feb-2013   Added font-mappings for symbols to handle superscripting END - rs -->
        </logical-font>

        <logical-font name="SansMediumLFRoman">
            <physical-font char-set="default">
                <font-face>DellReplica-Bold</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="SansBoldLFRoman">
            <physical-font char-set="default">
                <font-face>MetaBoldLF-Roman</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>TimesCyr</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="SansCondBoldRoman">
            <physical-font char-set="default">
                <font-face>DellReplica-Bold</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="SansMediumLFCaps">
            <physical-font char-set="default">
                <font-face>DellReplica-Bold</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="Monospaced">
            <physical-font char-set="default">
                <font-face>Courier New</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>Microsoft JhengHei</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="Wingdings">
            <physical-font char-set="default">
                <font-face>Wingdings</font-face>
            </physical-font>
        </logical-font>

        <!--There are no serif fonts in this project.  The following just "takes over" anything declared as serif.-->
        <logical-font name="Serif">
            <physical-font char-set="default">
                <font-face>MetaCondBook-Roman</font-face>
            </physical-font>
            <physical-font char-set="Japanese">
                <font-face>Meiryo UI</font-face>
            </physical-font>
            <physical-font char-set="Korean">
                <font-face>Malgun Gothic</font-face>
            </physical-font>
            <physical-font char-set="Russian">
                <font-face>Arial</font-face>
            </physical-font>
            <physical-font char-set="Simplified Chinese">
                <font-face>Microsoft YaHei</font-face>
            </physical-font>
            <physical-font char-set="Traditional Chinese">
                <font-face>MingLiU</font-face>
            </physical-font>
        </logical-font>

        <logical-font name="SansCondBoldRoman-Bold">
            <physical-font char-set="default">
                <font-face>Arial-Bold</font-face>
            </physical-font>
        </logical-font>
    </font-table>
</font-mappings>
